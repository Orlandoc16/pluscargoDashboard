# Streamlit Cloud Secrets Configuration
# This file should be configured in Streamlit Cloud's "Manage app" -> "Secrets" section
# DO NOT commit this file with real values to version control

[supabase]
# Your Supabase project URL
url = "https://your-project-id.supabase.co"

# Your Supabase anon key (public key)
anon_key = "your-supabase-anon-key-here"

# Your Supabase service role key (private key) - optional
service_role_key = "your-supabase-service-role-key-here"

[database]
# Database table names (optional - defaults will be used if not specified)
leads_table = "leads_pluscargo_simple"
call_results_table = "call_results_pluscargo_simple"
agents_table = "agents"

[app]
# Application settings (optional)
debug = false
cache_ttl = 300
max_retries = 3
timeout = 30

# Instructions for Streamlit Cloud deployment:
# 1. Go to your Streamlit Cloud app
# 2. Click "Manage app" in the lower right
# 3. Go to "Secrets" tab
# 4. Copy the [supabase] section above and replace with your actual values
# 5. Save and redeploy your app